{
    "extends": [
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/react-in-jsx-scope": "off",
      "semi": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": false,
          "endOfLine":"auto"
        }
      ]
    }
  }

   
